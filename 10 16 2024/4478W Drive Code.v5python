{"mode":"Text","hardwareTarget":"brain","textContent":"\n#region VEXcode Generated Robot Configuration\nfrom vex import *\nimport urandom\n\n# Brain should be defined by default\nbrain=Brain()\n\n# Robot configuration code\nmotor_1 = Motor(Ports.PORT16, GearSetting.RATIO_18_1, True)\nmotor_2 = Motor(Ports.PORT17, GearSetting.RATIO_18_1, True)\nmotor_3 = Motor(Ports.PORT11, GearSetting.RATIO_18_1, True)\nmotor_4 = Motor(Ports.PORT7, GearSetting.RATIO_18_1, False)\nmotor_5 = Motor(Ports.PORT5, GearSetting.RATIO_18_1, False)\nmotor_6 = Motor(Ports.PORT6, GearSetting.RATIO_18_1, False)\ncontroller_1 = Controller(PRIMARY)\narm = Motor(Ports.PORT21, GearSetting.RATIO_18_1, False)\ndigital_out_a = DigitalOut(brain.three_wire_port.a)\n\n\n# wait for rotation sensor to fully initialize\nwait(30, MSEC)\n\n\n# Make random actually random\ndef initializeRandomSeed():\n    wait(100, MSEC)\n    random = brain.battery.voltage(MV) + brain.battery.current(CurrentUnits.AMP) * 100 + brain.timer.system_high_res()\n    urandom.seed(int(random))\n      \n# Set random seed \ninitializeRandomSeed()\n\n\ndef play_vexcode_sound(sound_name):\n    # Helper to make playing sounds from the V5 in VEXcode easier and\n    # keeps the code cleaner by making it clear what is happening.\n    print(\"VEXPlaySound:\" + sound_name)\n    wait(5, MSEC)\n\n# add a small delay to make sure we don't print in the middle of the REPL header\nwait(200, MSEC)\n# clear the console to make sure we don't have the REPL in the console\nprint(\"\\033[2J\")\n\n#endregion VEXcode Generated Robot Configuration\n\n# ------------------------------------------\n# \n# \tProject:      4478W Drive Code\n#\tAuthor:       Theenash Muneeswaran, Grant Knect\n#\tCreated:      9/9/2024\n#\tDescription:  Drive code for the team 4478W made by Theenash Muneeswaran and Grant Knect\n# \n# ------------------------------------------\n\n# Library imports\nfrom vex import *\n\n# Begin project code\n\n#variables\nclicks_a = 0\nclicks_x = 0\n\nleft_motors = MotorGroup(motor_1, motor_2, motor_3)\nright_motors = MotorGroup(motor_4, motor_5, motor_6)\nall_mototrs = MotorGroup(motor_1, motor_2, motor_3, motor_4, motor_5, motor_6)\n\n#end of variables\n\n\n\n\n    \nif controller_1.buttonUp.pressed(TRUE):\n    controller_1.screen.clear_screen()\n    controller_1.screen.print(\"HI , WELCOME TO THE 4478F ROBOT MASTER COMMAND SYSTEM\")\n    \n\ncontroller_1.screen.print(\"AUTONOMUS MODE ACTIVATED\")\ncontroller_1.screen.print(\"RED RIGHT FOR UP ARROW\")\ncontroller_1.screen.next_row()\ncontroller_1.screen.print(\"RED LEFT FOR RIGHT ARROW\")\ncontroller_1.screen.next_row()\ncontroller_1.screen.print(\"BLUE RIGHT FOR DOWN ARROW\")\ncontroller_1.screen.next_row()\ncontroller_1.screen.print(\"BLUE LEFT FOR LEFT ARROW\")\ncontroller_1.screen.next_row()\n\n\n#autonomous  RED right\nif controller_1.buttonUp.pressed(TRUE):\n    controller_1.screen.clear_screen()\n    controller_1.screen.print(\"RED RIGHT NOW ACTIVATED\")\n    all_mototrs.spin_for(FORWARD, 1, TURNS)\n    arm.spin(FORWARD)\n    wait(1, SECONDS)\n    arm.stop()\n    right_motors.spin_for(FORWARD, 40, DEGREES) and left_motors.spin_for(REVERSE, 40, DEGREES)\n    arm.spin(REVERSE)\n    wait(1, SECONDS)\n    arm.stop()\n    arm.set_position(0, DEGREES)\n    right_motors.spin_for(REVERSE, 40, DEGREES) and left_motors.spin_for(FORWARD, 40, DEGREES)\n    all_mototrs.spin_for(FORWARD, 1, TURNS)\n    arm.spin(FORWARD)\n    wait(1, SECONDS)\n    arm.stop()\n    all_mototrs.spin_for(REVERSE, 1, TURNS)\n    right_motors.spin_for(FORWARD, 40, DEGREES) and left_motors.spin_for(REVERSE, 40, DEGREES)\n    all_mototrs.spin_for(FORWARD, 2, TURNS)\n    arm.spin(REVERSE)\n    wait(1, SECONDS)\n    arm.stop()\n    #scored 1\n    all_mototrs.spin_for(REVERSE, 2, TURNS)\n    right_motors.spin_for(REVERSE, 140, DEGREES) and left_motors.spin_for(FORWARD, 140, DEGREES)\n    all_mototrs.spin_for(FORWARD, 3, TURNS)\n    arm.spin(FORWARD)\n    wait(1, SECONDS)\n    arm.stop()\n    all_mototrs.spin_for(REVERSE, 1, TURNS)\n    right_motors.spin_for(REVERSE, 90, DEGREES) and left_motors.spin_for(FORWARD, 90, DEGREES)\n    all_mototrs.spin_for(FORWARD, 1, TURNS)\n    arm.spin(REVERSE)\n    wait(1, SECONDS)\n    arm.stop()\n    #scored 2\n    all_mototrs.spin_for(REVERSE, 1, TURNS)\n    right_motors.spin_for(REVERSE, 40, DEGREES) and left_motors.spin_for(FORWARD, 40, DEGREES)\n    all_mototrs.spin_for(REVERSE, 1, TURNS)\n    right_motors.spin_for(FORWARD, 90, DEGREES) and left_motors.spin_for(REVERSE, 90, DEGREES)\n    all_mototrs.spin_for(REVERSE, 90, DEGREES)\n\n\n\n\n\n\n\n#autonomous  RED left\nif controller_1.buttonRight.pressed(TRUE):\n    controller_1.screen.clear_screen()\n    controller_1.screen.print(\"RED LEFT NOW ACTIVATED\")\n    all_mototrs.spin_for(FORWARD, 1, TURNS)\n    arm.spin(FORWARD)\n    wait(1, SECONDS)\n    arm.stop()\n    right_motors.spin_for(REVERSE, 40, DEGREES) and left_motors.spin_for(FORWARD, 40, DEGREES)\n    arm.spin(REVERSE)\n    wait(1, SECONDS)\n    arm.stop()\n    arm.set_position(0, DEGREES)\n    right_motors.spin_for(FORWARD, 40, DEGREES) and left_motors.spin_for(REVERSE, 40, DEGREES)\n    all_mototrs.spin_for(FORWARD, 1, TURNS)\n    arm.spin(FORWARD)\n    wait(1, SECONDS)\n    arm.stop()\n    all_mototrs.spin_for(REVERSE, 1, TURNS)\n    right_motors.spin_for(REVERSE, 40, DEGREES) and left_motors.spin_for(FORWARD, 40, DEGREES)\n    all_mototrs.spin_for(FORWARD, 2, TURNS)\n    arm.spin(REVERSE)\n    wait(1, SECONDS)\n    arm.stop()\n    #scored 1\n    all_mototrs.spin_for(REVERSE, 2, TURNS)\n    right_motors.spin_for(FORWARD, 140, DEGREES) and left_motors.spin_for(REVERSE, 140, DEGREES)\n    all_mototrs.spin_for(FORWARD, 3, TURNS)\n    arm.spin(FORWARD)\n    wait(1, SECONDS)\n    arm.stop()\n    all_mototrs.spin_for(REVERSE, 1, TURNS)\n    right_motors.spin_for(REVERSE, 90, DEGREES) and left_motors.spin_for(FORWARD, 90, DEGREES)\n    all_mototrs.spin_for(FORWARD, 1, TURNS)\n    arm.spin(REVERSE)\n    wait(1, SECONDS)\n    arm.stop()\n    #scored 2\n    all_mototrs.spin_for(REVERSE, 1, TURNS)\n    right_motors.spin_for(FORWARD, 40, DEGREES) and left_motors.spin_for(REVERSE, 40, DEGREES)\n    all_mototrs.spin_for(REVERSE, 1, TURNS)\n    right_motors.spin_for(REVERSE, 90, DEGREES) and left_motors.spin_for(FORWARD, 90, DEGREES)\n    all_mototrs.spin_for(REVERSE, 90, DEGREES)\n\n\n\n\n\n\n#autonomous  BLUE right\nif controller_1.buttonDown.pressed(TRUE):\n    controller_1.screen.clear_screen()\n    controller_1.screen.print(\"BLUE RIGHT NOW ACTIVATED\")\n    all_mototrs.spin_for(FORWARD, 1, TURNS)\n    arm.spin(FORWARD)\n    wait(1, SECONDS)\n    arm.stop()\n    right_motors.spin_for(FORWARD, 40, DEGREES) and left_motors.spin_for(REVERSE, 40, DEGREES)\n    arm.spin(REVERSE)\n    wait(1, SECONDS)\n    arm.stop()\n    arm.set_position(0, DEGREES)\n    right_motors.spin_for(REVERSE, 40, DEGREES) and left_motors.spin_for(FORWARD, 40, DEGREES)\n    all_mototrs.spin_for(FORWARD, 1, TURNS)\n    arm.spin(FORWARD)\n    wait(1, SECONDS)\n    arm.stop()\n    all_mototrs.spin_for(REVERSE, 1, TURNS)\n    right_motors.spin_for(FORWARD, 40, DEGREES) and left_motors.spin_for(REVERSE, 40, DEGREES)\n    all_mototrs.spin_for(FORWARD, 2, TURNS)\n    arm.spin(REVERSE)\n    wait(1, SECONDS)\n    arm.stop()\n    #scored 1\n    all_mototrs.spin_for(REVERSE, 2, TURNS)\n    right_motors.spin_for(REVERSE, 140, DEGREES) and left_motors.spin_for(FORWARD, 140, DEGREES)\n    all_mototrs.spin_for(FORWARD, 3, TURNS)\n    arm.spin(FORWARD)\n    wait(1, SECONDS)\n    arm.stop()\n    all_mototrs.spin_for(REVERSE, 1, TURNS)\n    right_motors.spin_for(REVERSE, 90, DEGREES) and left_motors.spin_for(FORWARD, 90, DEGREES)\n    all_mototrs.spin_for(FORWARD, 1, TURNS)\n    arm.spin(REVERSE)\n    wait(1, SECONDS)\n    arm.stop()\n    #scored 2\n    all_mototrs.spin_for(REVERSE, 1, TURNS)\n    right_motors.spin_for(REVERSE, 40, DEGREES) and left_motors.spin_for(FORWARD, 40, DEGREES)\n    all_mototrs.spin_for(REVERSE, 1, TURNS)\n    right_motors.spin_for(FORWARD, 90, DEGREES) and left_motors.spin_for(REVERSE, 90, DEGREES)\n    all_mototrs.spin_for(REVERSE, 90, DEGREES)\n\n\n\n\n\n\n#autonomous  BLUE left\nif controller_1.buttonLeft.pressed(TRUE):\n    controller_1.screen.clear_screen()\n    controller_1.screen.print(\"BLUE LEFT NOW ACTIVATED\")\n    all_mototrs.spin_for(FORWARD, 1, TURNS)\n    arm.spin(FORWARD)\n    wait(1, SECONDS)\n    arm.stop()\n    right_motors.spin_for(REVERSE, 40, DEGREES) and left_motors.spin_for(FORWARD, 40, DEGREES)\n    arm.spin(REVERSE)\n    wait(1, SECONDS)\n    arm.stop()\n    arm.set_position(0, DEGREES)\n    right_motors.spin_for(FORWARD, 40, DEGREES) and left_motors.spin_for(REVERSE, 40, DEGREES)\n    all_mototrs.spin_for(FORWARD, 1, TURNS)\n    arm.spin(FORWARD)\n    wait(1, SECONDS)\n    arm.stop()\n    all_mototrs.spin_for(REVERSE, 1, TURNS)\n    right_motors.spin_for(REVERSE, 40, DEGREES) and left_motors.spin_for(FORWARD, 40, DEGREES)\n    all_mototrs.spin_for(FORWARD, 2, TURNS)\n    arm.spin(REVERSE)\n    wait(1, SECONDS)\n    arm.stop()\n    #scored 1\n    all_mototrs.spin_for(REVERSE, 2, TURNS)\n    right_motors.spin_for(FORWARD, 140, DEGREES) and left_motors.spin_for(REVERSE, 140, DEGREES)\n    all_mototrs.spin_for(FORWARD, 3, TURNS)\n    arm.spin(FORWARD)\n    wait(1, SECONDS)\n    arm.stop()\n    all_mototrs.spin_for(REVERSE, 1, TURNS)\n    right_motors.spin_for(REVERSE, 90, DEGREES) and left_motors.spin_for(FORWARD, 90, DEGREES)\n    all_mototrs.spin_for(FORWARD, 1, TURNS)\n    arm.spin(REVERSE)\n    wait(1, SECONDS)\n    arm.stop()\n    #scored 2\n    all_mototrs.spin_for(REVERSE, 1, TURNS)\n    right_motors.spin_for(FORWARD, 40, DEGREES) and left_motors.spin_for(REVERSE, 40, DEGREES)\n    all_mototrs.spin_for(REVERSE, 1, TURNS)\n    right_motors.spin_for(REVERSE, 90, DEGREES) and left_motors.spin_for(FORWARD, 90, DEGREES)\n    all_mototrs.spin_for(REVERSE, 90, DEGREES)\n\n\n#end of autonomous\ncontroller_1.screen.clear_screen()\ncontroller_1.screen.print(\"AUTONOMUS MODE NOW DEACTIVATED\")\ncontroller_1.screen.print(\"MANUAL CONTROL NOW ACTIVATED\")\n\n#right joystick\nif controller_1.axis2.position(100):\n    right_motors.spin_for(FORWARD, 3,TURNS)\n    \n\n\nif controller_1.axis2.position(-100):\n    right_motors.spin_for(REVERSE, 3,TURNS)\n\n\n#left joystick\n\nif controller_1.axis3.position(100):\n    left_motors.spin_for(FORWARD, 3,TURNS)\n\n\nif controller_1.axis3.position(-100):\n    left_motors.spin_for(REVERSE, 3,TURNS)\n\n\n'''\n\n#foward and backward\nif controller_1.axis2.position(100) and controller_1.axis3.position(100):\n    all_motors.spin_for(FORWARD, 3,TURNS)\n    \nif controller_1.axis2.position(-100) and controller_1.axis3.position(-100):\n    all_motors.spin_for(REVERSE, 3,TURNS)\n\n\n\n#right and left\nif controller_1.axis2.position(100) and controller_1.axis3.position(-100):\n    right_motors.spin_for(FORWARD, 3,TURNS) and left_motors.spin_for(REVERSE, 3,TURNS)\n    \nif controller_1.axis2.position(100) and controller_1.axis3.position(-100):\n    right_motors.spin_for(REVERSE, 3,TURNS) and left_motors.spin_for(FORWARD, 3,TURNS)\n\n'''\n\n\n# R1\n\n\nif controller_1.buttonR1.pressing():\n    arm.spin(FORWARD)\n\nif controller_1.buttonR2.pressing():\n    arm.spin(REVERSE)\n\n\n'''\n# R2\nif controller_1.buttonL1.pressing():\n    claw.spin(FORWARD)\n\nif controller_1.buttonL2.pressing():\n    claw.spin(REVERSE)\n\n\n'''\n\n\n\nif controller_1.buttonX.pressed():\n    clicks_x += 1\n\n\nif clicks_x % 2 == 0:\n    claw.spin_for(FORWARD, 30, DEGREES)\n\nelse:\n    claw.spin_for(REVERSE, 30, DEGREES)\n\n\n\n\n\n\n\n#Nuematics\n\n\n'''\nif controller_1.buttonA.pressed():\n    clicks_a += 1\n\n\n\nif clicks_a % 2 == 0:\n    digital_out_a.set(True)\n\nelse:\n    digital_out_a.set(False)\n\n'''\n","textLanguage":"python","robotConfig":[{"port":[16],"name":"motor_1","customName":true,"deviceType":"Motor","deviceClass":"motor","setting":{"reversed":"true","fwd":"forward","rev":"reverse","gear":"ratio18_1"},"triportSourcePort":22},{"port":[17],"name":"motor_2","customName":true,"deviceType":"Motor","deviceClass":"motor","setting":{"reversed":"true","fwd":"forward","rev":"reverse","gear":"ratio18_1"},"triportSourcePort":22},{"port":[11],"name":"motor_3","customName":true,"deviceType":"Motor","deviceClass":"motor","setting":{"reversed":"true","fwd":"forward","rev":"reverse","gear":"ratio18_1"},"triportSourcePort":22},{"port":[7],"name":"motor_4","customName":true,"deviceType":"Motor","deviceClass":"motor","setting":{"reversed":"false","fwd":"forward","rev":"reverse","gear":"ratio18_1"},"triportSourcePort":22},{"port":[5],"name":"motor_5","customName":false,"deviceType":"Motor","deviceClass":"motor","setting":{"reversed":"false","fwd":"forward","rev":"reverse","gear":"ratio18_1"},"triportSourcePort":22},{"port":[6],"name":"motor_6","customName":false,"deviceType":"Motor","deviceClass":"motor","setting":{"reversed":"false","fwd":"forward","rev":"reverse","gear":"ratio18_1"},"triportSourcePort":22},{"port":[],"name":"controller_1","customName":false,"deviceType":"Controller","deviceClass":"controller","setting":{"left":"","leftDir":"false","right":"","rightDir":"false","upDown":"","upDownDir":"false","xB":"","xBDir":"false","drive":"none","id":"primary"},"triportSourcePort":22},{"port":[21],"name":"arm","customName":true,"deviceType":"Motor","deviceClass":"motor","setting":{"reversed":"false","fwd":"forward","rev":"reverse","gear":"ratio18_1","id":"partner"},"triportSourcePort":22},{"port":[1],"name":"digital_out_a","customName":false,"deviceType":"DigitalOut","deviceClass":"digital_out","setting":{"id":"partner"},"triportSourcePort":22}],"slot":0,"platform":"V5","sdkVersion":"20240802.15.00.00","appVersion":"4.0.5","fileFormat":"2.0.0","targetBrainGen":"First","v5Sounds":[{"name":"game over","url":"static/sounds/mixkit-arcade-retro-game-over-213.wav"}],"v5SoundsEnabled":false}